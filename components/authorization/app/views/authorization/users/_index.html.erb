

  <div class="row">
    <div class="col-xs-6">
      <h4 class="pull-left">Users</h4>
    </div>
    <div class="col-xs-6">
      <%= link_to :new_user, :title => 'New User', :remote => true do %>
          <button type="" class="btn bg-navy margin-top pull-right">+Add User</button>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-body" id="table_content">
            <%= render :partial => 'table', :locals => {users: @users} %>
        </div>
      </div>
    </div>
  </div>

